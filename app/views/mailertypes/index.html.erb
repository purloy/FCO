<p id="notice"><%= notice %></p>

<h1>Listing Mailertypes</h1>

<table>
  <thead>
    <tr>
      <th>Mailer type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mailertypes.each do |mailertype| %>
      <tr>
        <td><%= mailertype.mailer_type %></td>
        <td><%= link_to 'Edit', edit_mailertype_path(mailertype) %></td>
        <td><%= link_to 'Destroy', mailertype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


