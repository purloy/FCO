<p id="notice"><%= notice %></p>


<table>
  <thead>
    <tr>
      <th>Mailer name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mailernames.each do |mailername| %>
      <tr>
        <td><%= mailername.mailer_name %></td>
        <td><%= link_to 'Show', mailername %></td>
        <td><%= link_to 'Edit', edit_mailername_path(mailername) %></td>
        <td><%= link_to 'Destroy', mailername, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mailername', new_mailername_path %>
